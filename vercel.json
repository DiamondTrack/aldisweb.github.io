{
  "trailingSlash": true
}
{
  "functions": {
    "quotes/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/quote/kutipan", "dest": "/quotes/kutipan/index.php" },
    { "src": "/quote/diary", "dest": "/quotes/diary/index.php" },
    { "src": "/quote/motivasi", "dest": "/quotes/motivasi/index.php" },
    { "src": "/quote/(.*)", "dest": "/quotes/$1" }
  ]
}
